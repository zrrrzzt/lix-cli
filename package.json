  "main": "index.js",
  "bin": {
    "lix": "index.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "test": "standard && npm audit && tap --reporter=spec test/*.js",
